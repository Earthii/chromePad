{
  "name": "chromePad",
  "short_name": "chromePad",
  "version": "1.0.7",
  "description":
    "A chrome extension enables users to sync their notes to chrome. App oowered by an rich text editor",
  "browser_action": {
    "default_icon": "assets/icon.png",
    "default_popup": "index.html"
  },
  "content_security_policy":
    "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": ["storage"],
  "manifest_version": 2
}
